#!/usr/bin/env node

var program    = require('commander'),
    pedaMaster = require('../lib/index.js')
    pkg        = require('../package.json');
    
    
program
  .version(pkg.version)
  .option("-p, --port [63532]", "The port [63532]", "63532")
  .parse(process.argv);


pedaMaster.init(parseInt(program.port));

pedaMaster.start();
